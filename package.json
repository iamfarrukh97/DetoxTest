{
  "name": "DetoxPractice",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "detox:clean:cache": "detox clean-framework-cache && detox build-framework-cache",
    "detox:ios:debug:build": "detox build --configuration ios.sim.debug",
    "detox:ios:debug:test": "yarn detox:clean:cache && detox test --configuration ios.sim.debug",
    "detox:ios:release:build": "detox build --configuration ios.sim.release",
    "detox:ios:release:test": "detox test --configuration ios.sim.release",
    "detox:android:debug:build": "detox build --configuration android.emu.debug",
    "detox:android:debug:test": "yarn detox:clean:cache && detox test --configuration android.emu.debug",
    "detox:android:release:build": "detox build --configuration android.emu.release",
    "detox:android:release:test": "detox test --configuration android.emu.release",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/datetimepicker": "^6.7.1",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-native-picker/picker": "^2.4.8",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/stack": "^6.3.10",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "moment": "^2.29.4",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-calendars": "^1.1292.0",
    "react-native-date-picker": "^4.2.6",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-maps": "^1.3.2",
    "react-native-reanimated": "^2.13.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "detox": "^20.1.0",
    "eslint": "^7.32.0",
    "jest": "^29",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
